package Test.ss;

public class reverse {

	 public static void main(String[] args){  
		 	String s = "This is a sample test";
		 	char ch[]=s.toCharArray();  
		    String rev="";  
		    for(int i=ch.length-1;i>=0;i--){  
		        rev+=ch[i]; 
		        System.out.println(rev);
		}  
	        
	    }  
}
