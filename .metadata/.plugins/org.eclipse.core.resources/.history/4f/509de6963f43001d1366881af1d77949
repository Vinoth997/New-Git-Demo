package Main.ss;

public class power {

	public static void main(String[] args) {
	
		int a[] = {1,2,3,4,5,3,2,3,4,1};
		
		
		for(int i=0;i<a.length;i++) {
			for(int j=0;j<a.length;j++) {
				if(a[i]==a[j]) {
					int count = 0;
				    count++;
				    System.out.println(a[i]+" : "+count);
				}
			}
			
		}
		

	}

}
